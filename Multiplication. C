include <stdio.h>

// Function declaration 
int multiply(int a, int b);

int main() {
    int num1, num2, result;

    // Input from user
    printf("Enter two numbers to multiply: ");
    scanf("%d %d", &num1, &num2);

    // Function call
    result = multiply(num1, num2);

    // Output the result
    printf("Result: %d x %d = %d\n", num1, num2, result);

    return 0;
}

// Function definition
int multiply(int a, int b) {
    return a * b;
}
